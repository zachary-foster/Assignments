	keywords = {ecology, ecology, {Geoecology/Natural} Processes, {Geoecology/Natural} Processes, Microbial Ecology, Microbial Ecology, Microbiology, Microbiology, Nature Conservation, Nature Conservation},
	pages = {157--166},
