	keywords = {Genomes, Genomes, Molecular markers, Molecular markers, Oomycetes, Oomycetes, Phylogeny, Phylogeny, Phytophthora infestans, Phytophthora infestans, Phytophthora ramorum, Phytophthora ramorum, Phytophthora sojae, Phytophthora sojae},
