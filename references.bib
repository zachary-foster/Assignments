	year = {2009},
	keywords = {454 pyrosequencing, 454 pyrosequencing, community structure, community structure, ectomycorrhizal fungi, ectomycorrhizal fungi, environmental samples, environmental samples, fungal diversity, fungal diversity, nuclear ribosomal internal transcribed spacers, nuclear ribosomal internal transcribed spacers},
