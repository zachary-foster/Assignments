	pages = {918–928},
	file = {Full Text PDF:C:\Users\Zachary Foster\AppData\Roaming\Zotero\Zotero\Profiles\y5722wyr.default\zotero\storage\9BU2GMBI\Giessler and Wolinska - 2013 - Capturing the population structure of microparasit.pdf:application/pdf;Snapshot:C:\Users\Zachary Foster\AppData\Roaming\Zotero\Zotero\Profiles\y5722wyr.default\zotero\storage\DH9UEHJ5\abstract.html:text/html}
},

